<h1>Restaurants</h1>
<ul class="restaurants">
    {{#each restaurants}}
    <li class="restaurants-item" >
        <a href="/apis/restaurants/{{this.id}}" target="_blank">
        <img src="{{this.imageURL}}" class="restaurants-item-image">
        <div class="restaurants-item-name">{{this.name}}</div>
        <small class="restaurants-item-type">{{this.type}}</small>
        </a>
    </li>
    {{/each}}
</ul>